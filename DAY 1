# 6. Check whether a number is positive, negative or zero
print("6. Positive, Negative or Zero Check:")
num = float(input("Enter a number: "))

if num > 0:
    print("The number is positive")
elif num < 0:
    print("The number is negative")
elif num == 0:
    print("The number is zero")

print()
print("=" * 50)
print()

# 7. Find roots of quadratic equation
print("7. Roots of Quadratic Equation:")
print("For equation ax² + bx + c = 0")
a = float(input("Enter coefficient a: "))
b = float(input("Enter coefficient b: "))
c = float(input("Enter coefficient c: "))

# Calculate discriminant
discriminant = (b * b) - (4 * a * c)

if discriminant > 0:
    print("Roots are real and different")
    root1 = (-b + (discriminant ** 0.5)) / (2 * a)
    root2 = (-b - (discriminant ** 0.5)) / (2 * a)
    print("Root 1 =", root1)
    print("Root 2 =", root2)
elif discriminant == 0:
    print("Roots are real and equal")
    root = -b / (2 * a)
    print("Root =", root)
elif discriminant < 0:
    print("Roots are complex")
    real_part = -b / (2 * a)
    imaginary_part = ((-discriminant) ** 0.5) / (2 * a)
    print("Root 1 =", real_part, "+", imaginary_part, "i")
    print("Root 2 =", real_part, "-", imaginary_part, "i")

print()
print("=" * 50)
print()

# 8. Simple Calculator using switch case
print("8. Simple Calculator:")
print("1. Addition")
print("2. Subtraction")
print("3. Multiplication")
print("4. Division")

choice = int(input("Enter your choice (1-4): "))
num1 = float(input("Enter first number: "))
num2 = float(input("Enter second number: "))

if choice == 1:
    result = num1 + num2
    print("Result:", num1, "+", num2, "=", result)
elif choice == 2:
    result = num1 - num2
    print("Result:", num1, "-", num2, "=", result)
elif choice == 3:
    result = num1 * num2
    print("Result:", num1, "*", num2, "=", result)
elif choice == 4:
    if num2 != 0:
        result = num1 / num2
        print("Result:", num1, "/", num2, "=", result)
    else:
        print("Error: Division by zero is not allowed!")
else:
    print("Invalid choice!")

print()
print("Program completed!")
