IF ELSE QUESTIONS
# 1. Program to find maximum between two numbers
print("Program 1: Maximum between two numbers")
a = float(input("Enter first number: "))
b = float(input("Enter second number: "))

if a > b:
    print(f"Maximum is: {a}")
else:
    print(f"Maximum is: {b}")

print("\n" + "="*50 + "\n")

# 2. Program to find maximum between three numbers
print("Program 2: Maximum between three numbers")
x = float(input("Enter first number: "))
y = float(input("Enter second number: "))
z = float(input("Enter third number: "))

if x >= y and x >= z:
    print(f"Maximum is: {x}")
else:
    if y >= z:
        print(f"Maximum is: {y}")
    else:
        print(f"Maximum is: {z}")

print("\n" + "="*50 + "\n")

# 3. Program to check whether a number is negative, positive or zero
print("Program 3: Check number sign")
num = float(input("Enter a number: "))

if num > 0:
    print("Number is positive")
else:
    if num < 0:
        print("Number is negative")
    else:
        print("Number is zero")

print("\n" + "="*50 + "\n")

# 4. Program to check whether a number is divisible by 5 and 11
print("Program 4: Check divisibility by 5 and 11")
num = int(input("Enter a number: "))

if num % 5 == 0 and num % 11 == 0:
    print("Number is divisible by both 5 and 11")
else:
    if num % 5 == 0:
        print("Number is divisible by 5 but not by 11")
    else:
        if num % 11 == 0:
            print("Number is divisible by 11 but not by 5")
        else:
            print("Number is not divisible by 5 or 11")

print("\n" + "="*50 + "\n")

# 5. Program to check whether a number is even or odd
print("Program 5: Check even or odd")
num = int(input("Enter a number: "))

if num % 2 == 0:
    print("Number is even")
else:
    print("Number is odd")

print("\n" + "="*50 + "\n")

# 6. Program to check whether a year is leap year or not
print("Program 6: Check leap year")
year = int(input("Enter a year: "))

if year % 4 == 0:
    if year % 100 == 0:
        if year % 400 == 0:
            print("Leap year")
        else:
            print("Not a leap year")
    else:
        print("Leap year")
else:
    print("Not a leap year")

print("\n" + "="*50 + "\n")

# 7. Program to check whether a character is alphabet or not
print("Program 7: Check alphabet")
char = input("Enter a character: ")

if len(char) == 1:
    if (char >= 'a' and char <= 'z') or (char >= 'A' and char <= 'Z'):
        print("Character is an alphabet")
    else:
        print("Character is not an alphabet")
else:
    print("Please enter only one character")

print("\n" + "="*50 + "\n")

# 8. Program to check whether an alphabet is vowel or consonant
print("Program 8: Check vowel or consonant")
char = input("Enter an alphabet: ")

if len(char) == 1:
    if (char >= 'a' and char <= 'z') or (char >= 'A' and char <= 'Z'):
        if char == 'a' or char == 'e' or char == 'i' or char == 'o' or char == 'u' or char == 'A' or char == 'E' or char == 'I' or char == 'O' or char == 'U':
            print("Vowel")
        else:
            print("Consonant")
    else:
        print("Please enter an alphabet")
else:
    print("Please enter only one character")

print("\n" + "="*50 + "\n")

# 9. Program to check whether a character is alphabet, digit or special character
print("Program 9: Check character type")
char = input("Enter a character: ")

if len(char) == 1:
    if (char >= 'a' and char <= 'z') or (char >= 'A' and char <= 'Z'):
        print("Alphabet")
    else:
        if char >= '0' and char <= '9':
            print("Digit")
        else:
            print("Special character")
else:
    print("Please enter only one character")

print("\n" + "="*50 + "\n")

