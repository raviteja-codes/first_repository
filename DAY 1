char = input("Enter an alphabet: ")

if char == 'a' or char == 'A':
    print("It is a vowel")
elif char == 'e' or char == 'E':
    print("It is a vowel")
elif char == 'i' or char == 'I':
    print("It is a vowel")
elif char == 'o' or char == 'O':
    print("It is a vowel")
elif char == 'u' or char == 'U':
    print("It is a vowel")
else:
    print("It is a consonant")

print()
