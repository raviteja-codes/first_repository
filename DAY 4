7.
n = int(input())
b = n
a = 0
while(b!=0):
    p = b%10
    if(a<p):
        a=p
    q = b//10
    b=q
d=a
c=0
p=0
q=0
a=n
tot=0
rev=0
while(n!=0):
    r=n%10
    n=n//10
    rev=rev*10+r
while(rev>0):
        p = rev//d
        q = rev%d
        tot += (q*(10**c))
        c+=1
        rev=p
print(tot)

8.
n = int(input())
o= n
s = 0
while n > 0:
    d = n % 10
    f = 1
    i = 1
    while i <= d:
        f *= i
        i += 1
    
    s += f
    n //= 10
if s == o:
    print(f"{original_num} is a strong number.")
else:
    print(f"{original_num} is not a strong number.")

9.
n = int(input())
s=0
while(n>=10):
    p = n%100
    a=p
    rev=0
    while(a!=0):
        r=a%10
        rev=rev*10+r
        a=a//10
    s += rev
    n=n//10
print(s)

10.
n = int(input())
s=0
while(n>0):
    p = n%10
    if(p%2!=0):
        f = 1
        for i in range(1,p+1):
            f*=i
        s+=f
    n=n//10
print(s)

