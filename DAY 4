n = int(input())
sum=0
while(n>=10):
    r = n%100
    sum+=r
    q = n//10
    n=q
print(sum)

2.
n = int(input())
sum=0
for i in range(n,0,-1):
    sum+=i
print(sum)

3.
n = int(input())
sum=1
for i in range(n,0,-1):
    sum*=i
print(sum)

4.
n=int(input())
d=0
b=0
while(n>0):
    p = n//2
    q = n%2
    b += (q*(10**d))
    d+=1
    n=p
print(b)

5.
import math
n=int(input())
g = int(input())
d = int(input())
z=int(math.log10(n))+1
count=0
c=0
tem=n
a=0
while(n!=0):
    e = n%10
    f = n//10
    if(g>e):
        count+=1
    n = f
n=tem
if(count==z):
    while(n>0):
        p = n//d
        q = n%d
        a += (q*(g**c))
        c+=1
        n=p
    print(a)
else:
    print("error")

